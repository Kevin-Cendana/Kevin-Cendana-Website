effect\n    useEffect(() => {\n        if (preloadImageSrc) {\n            const link = document.createElement('link');\n            link.rel = 'preload';\n            link.as = 'image';\n            link.href = preloadImageSrc;\n            document.head.appendChild(link);\n\n            return () => document.head.removeChild(link);\n        }\n    }, [preloadImageSrc]);\n\n\n    // Determine the class for text container based on states\n    const textContainerClass = classNames({      \n        'home__text-container': true,\n        'animate-home-text-container': isHomeInView,\n        'dark-mode': isDarkMode \n    });\n\n    // Render the Home section\n    return (\n        <section className = \"home\" id = \"home\" ref={homeRef}>\n            {/* Left side of the Home section w/ my name & rotating titles */}\n            <div className=\"home__top\">\n                <div className=\"home__left\">\n                    <div className={textContainerClass}>\n                        <div className =\"text-wrapper\">\n                            {/* <h1 className=\"home__text-name\">Hi, my name is</h1>\n                            <h1 className=\"home__text-name\">Kevin Cendana.</h1>\n                            <h1 className=\"home__text-name\">I'm a programmer.</h1> */}\n                            <h1 className=\"home__text-name\">Kevin Cendana.</h1>\n                            <h1 className=\"home__text-name\">Software Engineer.</h1>\n                            <h1 className=\"home__text-name\">Web Developer.</h1>\n                            <div className=\"home__text-alias\">\n                                <p>Passionate about creating seamless user experiences, </p>\n                                <p>I take pride in myself for writing clean and efficient code.</p>\n                                <p>I strive to make projects functional and visually appealing.</p>\n                            </div>\n                            <div class=\"home__button-wrapper\">\n                            <ContactMeButton />\n                            <ViewResumeButton />\n                        </div>\n                        </div>\n\n                    </div>\n                </div>\n                {/* Right side of the Home section w/ drawing of me, darkens on dark mode*/}\n                <img src={imageSrc}\n                    onError={handleError}\n                    alt='Kevin Cendana Drawing'\n                    className={`home-image ${isHomeInView ? 'animate-home-image' : ''}`}\n                    draggable=\"false\">\n                </img>\n                {/* Hero background image */}\n                <img src={heroBackground}\n                    alt='Hero Background'\n                    className={`hero-background ${isHomeInView ? 'animate-hero-background' : ''}`}\n                    draggable=\"false\">\n                </img>\n            </div>\n            <div className=\"home__bottom\">\n            </div>\n        </section>\n    );\n}\n\nexport default Home;\n","/Users/home/Desktop/Workspace/Projects HTML CSS JS/Kevin-Cendana-Website/src/components/About/About.js",[],[],"/Users/home/Desktop/Workspace/Projects HTML CSS JS/Kevin-Cendana-Website/src/components/Footer/Footer.js",[],[],"/Users/home/Desktop/Workspace/Projects HTML CSS JS/Kevin-Cendana-Website/src/components/Projects/Projects.js",[],[],"/Users/home/Desktop/Workspace/Projects HTML CSS JS/Kevin-Cendana-Website/src/components/Contact/Contact.js",["103","104"],[],"/Users/home/Desktop/Workspace/Projects HTML CSS JS/Kevin-Cendana-Website/src/components/Skills/Skills.js",[],[],"/Users/home/Desktop/Workspace/Projects HTML CSS JS/Kevin-Cendana-Website/src/shared/DarkModeToggle/CookieUtils.js",[],[],"/Users/home/Desktop/Workspace/Projects HTML CSS JS/Kevin-Cendana-Website/src/shared/HoverImage.js",[],[],"/Users/home/Desktop/Workspace/Projects HTML CSS JS/Kevin-Cendana-Website/src/shared/ContactMeButton.js",[],[],"/Users/home/Desktop/Workspace/P